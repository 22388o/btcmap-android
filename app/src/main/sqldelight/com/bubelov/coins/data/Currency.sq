CREATE TABLE Currency (
    id TEXT PRIMARY KEY,
    name TEXT NOT NULL,
    code TEXT NOT NULL,
    crypto INTEGER as Boolean NOT NULL,
    createdAt TEXT NOT NULL,
    updatedAt TEXT NOT NULL
);

insertOrReplace:
INSERT OR REPLACE
INTO Currency
VALUES ?;

selectAll:
SELECT *
FROM Currency;

selectById:
SELECT *
FROM Currency
WHERE id = ?;

selectCount:
SELECT COUNT(*)
FROM Currency;

selectMaxUpdatedAt:
SELECT MAX(updatedAt)
FROM Currency;